/**index.wxss**/
@import "../../common/style/common.wxss";
.container{
  width:710rpx;
  margin:0 auto;
}
/* 头部定位部分 */
.location-user{
  padding-top:26rpx;
  height:60rpx;
}
.location{
  height:60rpx;
}
.location-icon1{
  width:32rpx;
  height:36rpx;
  margin-right:6rpx;
}
.location text{
  font-size:18px;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(58,58,58,1);
}
.location-icon2{
  width:16rpx;
  height:32rpx;
  margin-left:10rpx;
}
.user{
  position: relative;
  width:156rpx;
  height:60rpx;
  background:rgba(255,255,255,1);
  border-radius:30rpx;
  border:1rpx solid rgba(233,236,243,1);
}
.user.hasRed::after{
  content:'';
  position: absolute;
  right:6rpx;
  top:-2rpx;
  width:16rpx;
  height:16rpx;
  border:none;
  border-radius: 100%;
  background:rgba(255,68,62,1);
}
.user-icon{
  width:34rpx;
  height:36rpx;
  margin-right:8rpx;
}
.user text{
  font-size:15px;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(58,58,58,1);
}
/* 搜索框 */
.search-box.sticky{
  position:fixed;
  top:176rpx;
  left:0;
  width:100%;
  z-index:1;
}
.search-box{
  padding: 24rpx 0 20rpx;
  background: #fff;
}
.search{
  width:408rpx;
  height:40rpx;
  padding:14rpx 0 14rpx 302rpx;
  margin:0 auto;
  background:rgba(247,247,247,1);
  border-radius:40rpx;
  background-image: url(data:image/png;base64,);
  background-size: auto 32rpx;
  background-position: 260rpx center;
  background-repeat: no-repeat;
  font-size:14px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(193,193,193,1);
}
/* 部分 */
.content.sticky{
  /* position:fixed;
  top:209rpx;
  left:0;
  width:100%; */
}
.content-vertical{
  width:180rpx;
  background:rgba(244,244,244,1);
}
.content-item{
  height:106rpx;
  border-bottom: 1rpx solid rgba(237, 237, 237, 0.5);
}
.content-item.active{
  position: relative;
  background-color:#fff;
  border:none;
}
.content-item.active::before{
  content: '';
  position: absolute;
  top:50%;
  left:0;
  margin-top:-20rpx;
  width:4rpx;
  height:40rpx;
  background-color: rgba(255, 68, 62, 1);
}
.content-item.active text{
  font-size:15px;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(255,68,62,1);
}
.content-item .content-icon1{
  width:30rpx;
  height:40rpx;
  margin-right:24rpx;
}
.content-item .content-icon2{
  width:38rpx;
  height:38rpx;
  margin-right: 18rpx;
}
.content-item text{
  max-width:132rpx;
  font-size:15px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(160,160,160,1);
  line-height:21px;
  text-align: center;
}
.content-list{
  flex:1;
}
.nav-tab{
  width:464rpx;
  height:106rpx;
  padding-left:18rpx;
}
.nav-tab view{
  flex:1;
  font-size:14px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(204,204,204,1);
  line-height:20px;
}
.nav-tab .active{
  color:rgba(27,27,27,1);
}
/* 菜单列表 */
.list{
  padding-bottom:176rpx;
  padding-left:20rpx;
}
.list-item{
  margin-bottom:18rpx;
}
.item-img{
  margin-right:18rpx;
  border-radius:4rpx;
  border:1rpx solid rgba(244,244,244,1);
  width:140rpx;
  height:140rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100%;
  background-position: center;
  background-repeat: no-repeat;
}
.item-right{
  padding:4rpx 36rpx 10rpx 0;
  flex:1;
  border-bottom: 1rpx solid rgba(236, 239, 244, 1);
}
.detail-title{
  width:274rpx;
  height:84rpx;
  font-size:15px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(0,0,0,1);
  line-height:21px;
}
.detail-txt{
  margin-top:14rpx;
  height:25px;
  font-size:18px;
  font-family:PingFangSC-Semibold,PingFang SC;
  font-weight:600;
  color:rgba(255,68,62,1);
  line-height:25px;
}
.detail-weight{
  font-size:12px;
  color:rgba(178, 178, 178, 1);
  margin-left:10rpx;
}
.detail-del{
  font-size:13px;
  color:rgba(178,178,178,1);
  margin-left:20rpx;
  text-decoration:line-through;
}
.item-right image{
  width:50rpx;
  height:50rpx;
  margin-left:32rpx;
}
/* 底部结算入口 */
.bottom{
  padding:0 40rpx 30rpx 22rpx;
  height:146rpx;
  background: #fff;
}
.bottom-img{
  position: relative;
  width:136rpx;
  height:122rpx;
  margin-right:20rpx;
}
.bottom-icon{
  top:10rpx;
  right:12rpx;
  width:38rpx;
  height:38rpx;
  background:rgba(255,70,35,1);
  font-size:11px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
  border-radius: 100%;
}
.bottom-price-box{
  height:25px;
  font-size:18px;
  font-family:PingFangSC-Semibold,PingFang SC;
  font-weight:600;
  color:rgba(255,68,62,1);
  line-height:25px;
}
.price-del{
  margin-left:22rpx;
  height:18px;
  font-size:13px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(201,201,201,1);
  line-height:18px;
}
.bottom-txt{
  margin-top:14rpx;
  height:18px;
  font-size:13px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
  line-height:18px;
}
.bottom-btn{
  width:88px;
  height:35px;
  background:linear-gradient(315deg,rgba(255,122,103,1) 0%,rgba(255,68,62,1) 100%);
  border-radius:17px;
  font-size:15px;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
}